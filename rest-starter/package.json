{
  "name": "keeno-rest-starter",
  "version": "0.1.0",
  "description": "A REST API starter built with keeno packages",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon index.new.js",
    "debug": "NODE_ENV=debug nodemon index.js",
    "encrypt": "./_encrypt_all.sh",
    "test": "NODE_ENV=test node --test ./test",
    "tests": "NODE_ENV=test node --test ./tests"
  },
  "keywords": [
    "rest",
    "starter",
    "keeno"
  ],
  "author": "Mike Terry",
  "license": "MIT",
  "dependencies": {
    "keeno-base": "workspace:*",
    "keeno-env": "workspace:*",
    "keeno-mongodb": "workspace:*",
    "keeno-nodemailer": "workspace:*",
    "keeno-rest": "workspace:*",
    "keeno-schema": "workspace:*",
    "keeno-system": "workspace:*"
  },
  "devDependencies": {
    "keeno-test": "workspace:*"
  }
}
