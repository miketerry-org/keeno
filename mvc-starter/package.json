{
  "name": "keeno-mvc-starter",
  "version": "0.1.0",
  "description": "A MVC application starter built with keeno packages",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon index.js",
    "debug": "NODE_ENV=debug nodemon index.js",
    "encrypt": "./_encrypt_all.sh",
    "test": "NODE_ENV=test node --test ./test",
    "tests": "NODE_ENV=test node --test ./tests",
    "test:server": "NODE_ENV=test nodemon --watch ../rest --watch ../schema --watch index.js --ext js,json index.js"
  },
  "keywords": [
    "mvc",
    "starter",
    "keeno"
  ],
  "author": "Mike Terry",
  "license": "MIT",
  "dependencies": {
    "keeno-base": "workspace:*",
    "keeno-env": "workspace:*",
    "keeno-mongodb": "workspace:*",
    "keeno-mvc": "workspace:*",
    "keeno-nodemailer": "workspace:*",
    "keeno-schema": "workspace:*",
    "keeno-system": "workspace:*"
  },
  "devDependencies": {
    "keeno-test": "workspace:*"
  }
}
